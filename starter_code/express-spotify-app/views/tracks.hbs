 <section class="albums-container">


{{#each myTracks.items}}
<div class="track-row">
  <div>
    {{this.name}}
  </div>
  <div>
    <audio controls>
    <source src="{{this.preview_url}}" type="audio/ogg">
    <source src="{{this.preview_url}}" type="audio/mpeg"> Your browser does not support the audio element.
  </audio>
  </div>
  {{/each}}

  </section>